{
  "psi-header.variables": [["projectCreationYear", "2021"]],
  "psi-header.templates": [
    {
      "language": "*",
      "template": [
        "File: <<filename>>",
        "Project: <<projectname>>",
        "Created: <<filecreated('dddd, Do MMMM YYYY h:mm:ss a')>>",
        "Last Modified: <<dateformat('dddd, Do MMMM YYYY h:mm:ss a')>>",
        "Copyright Â© <<projectCreationYear>> My Custom Life"
      ]
    }
  ],
  "psi-header.config": { "forceToTop": true, "blankLinesAfter": 1 },
  "psi-header.changes-tracking": { "isActive": true },
  "editor.codeActionsOnSave": null
}
