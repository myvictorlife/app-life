{
  "name": "LIFE",
  "description": "LIFE project",
  "version": "0.0.1",
  "author": "Victor Cesar",
  "homepage": "https://mycustomlife.com.br/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ios:staging": "ionic capacitor build -c=staging ios",
    "build:prod": "ionic build -c=production",
    "test": "jest",
    "test:coverage": "npx jest --silent --ci --coverage --coverageReporters='text' --coverageReporters='text-summary'",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-ios": "npm run build:ios:staging && ionic capacitor sync ios",
    "open:ios": "npm run build-ios && ionic capacitor open ios",
    "build:android": "ionic capacitor prepare android",
    "pretty-quick": "npx pretty-quick --staged",
    "precommit": "npm run pretty-quick && ng lint && npm run test:ci",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@angular/core": "~12.1.5",
    "@angular/forms": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@capacitor/android": "3.3.1",
    "@capacitor/app": "^1.0.6",
    "@capacitor/core": "3.3.1",
    "@capacitor/haptics": "^1.1.3",
    "@capacitor/ios": "3.3.2",
    "@capacitor/keyboard": "^1.1.3",
    "@capacitor/status-bar": "^1.0.6",
    "@ionic-native/firebase": "^5.36.0",
    "@ionic/angular": "^5.5.2",
    "@ngrx/effects": "^12.5.1",
    "@ngrx/store": "^12.5.1",
    "@ngrx/store-devtools": "^12.5.1",
    "cordova-plugin-firebase": "^2.0.5",
    "prettier": "^2.4.1",
    "rxjs": "~6.6.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.0",
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "^12.1.5",
    "@angular/language-service": "~12.0.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.0",
    "@capacitor/cli": "3.3.1",
    "@ionic/angular-toolkit": "^5.0.0",
    "@ionic/lab": "3.2.10",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "codecov": "^3.8.3",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-ngrx": "^1.47.0",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^7.0.4",
    "jasmine-marbles": "^0.8.4",
    "jest": "^27.3.1",
    "jest-preset-angular": "^11.0.0-rc.1",
    "lint-staged": "^12.0.2",
    "nyc": "^15.1.0",
    "protractor": "~7.0.0",
    "ts-jest": "^27.0.7",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4"
  },
  "husky": {
    "pre-commit": "npm run lint",
    "pre-push": "npm run test"
  },
  "nyc": {
    "extension": [
      ".tsx",
      ".ts"
    ],
    "include": [
      "src/**/*.ts?(x)"
    ],
    "exclude": [
      "**/*.test.ts?(x)",
      "**/*.d.ts",
      "**/*.js"
    ]
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/jest.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "^@life-store$": "<rootDir>/src/app/core/store/index.ts",
      "^@life-store/(.*)$": "<rootDir>/src/app/core/store/$1",
      "^config/lang$": "<rootDir>/src/config/lang",
      "^environments/environment$": "<rootDir>/src/environments/environment",
      "^@life-service(.*)$": "<rootDir>/src/app/shared/services/$1",
      "^@life-components(.*)$": "<rootDir>/src/app/components/$1",
      "^@life-shared(.*)$": "<rootDir>/src/app/shared/$1"
    }
  }
}
